<h1 class="jams-new-h1">Write your Review</h1>
<div class="container jams-new">
  <div class="row">
    <div class="col-xs-12 col-md-6 col-sm-offset-3">
      <div class="form-login2 margin-top">
      <p><%= simple_form_for [@jamm, @review] do |f| %></p>
      <div class="form-inputs">
      <p><%= f.input :title %></p>
      <p><%= f.input :content %></p>
      <p><%= f.input :rating %></p>
      <div class="form-actions">
      <p><%= f.submit "Send", class: "btn btn-primary"%></p>
      </div>
    <!--   <p>I NEED THIS PARAGRAPH TO MAKE THE SUBMIT BUTTON SHOWN</p> -->
      <% end %>
      </div>
   </div>
  </div>
</div>
